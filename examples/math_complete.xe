// Exponential growth
fn exponentialGrowth(initial, rate, time) {
    const exponent = rate * time
    return initial * math.exp(exponent)
}

const growth = exponentialGrowth(1000, 0.05, 10)
print("Population growth:", growth)
print()

// Distance calculation
fn distance(x1, y1, x2, y2) {
    const dx = x2 - x1
    const dy = y2 - y1
    return math.hypot(dx, dy)
}

const dist = distance(0, 0, 3, 4)
print("Distance:", dist)
print()

// Catenary curve (hanging chain)
fn catenary(x, a) {
    return a * cosh(x / a)
}

const curve = catenary(1, 1)
print("Catenary:", curve)
print()

