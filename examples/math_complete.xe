// ============================================================================
// HYPERBOLIC FUNCTIONS
// ============================================================================

print("4. Hyperbolic Functions")
print()

// sinh(x) = (e^x - e^-x) / 2
fn sinh(x) {
    const ex = math.exp(x)
    const emx = math.exp(-x)
    return (ex - emx) / 2
}

// cosh(x) = (e^x + e^-x) / 2
fn cosh(x) {
    const ex = math.exp(x)
    const emx = math.exp(-x)
    return (ex + emx) / 2
}

// tanh(x) = sinh(x) / cosh(x)
fn tanh(x) {
    const s = sinh(x)
    const c = cosh(x)
    return s / c
}

const sinh0 = sinh(0)
const sinh1 = sinh(1)
const cosh0 = cosh(0)
const cosh1 = cosh(1)
const tanh0 = tanh(0)
const tanh1 = tanh(1)

print("sinh(0):", sinh0)
print("sinh(1):", sinh1)
print("cosh(0):", cosh0)
print("cosh(1):", cosh1)
print("tanh(0):", tanh0)
print("tanh(1):", tanh1)
print()

// ============================================================================
// INVERSE HYPERBOLIC FUNCTIONS
// ============================================================================

print("5. Inverse Hyperbolic Functions")
print()

// asinh(x) = ln(x + sqrt(x^2 + 1))
fn asinh(x) {
    const x2 = x * x
    const sqrt_part = math.sqrt(x2 + 1)
    return math.log(x + sqrt_part)
}

// acosh(x) = ln(x + sqrt(x^2 - 1))
fn acosh(x) {
    const x2 = x * x
    const sqrt_part = math.sqrt(x2 - 1)
    return math.log(x + sqrt_part)
}

// atanh(x) = ln((1+x)/(1-x)) / 2
fn atanh(x) {
    const num = 1 + x
    const den = 1 - x
    return math.log(num / den) / 2
}

const asinh0 = asinh(0)
const asinh1 = asinh(1)
const acosh1 = acosh(1)
const acosh2 = acosh(2)
const atanh0 = atanh(0)
const atanh_half = atanh(0.5)

print("asinh(0):", asinh0)
print("asinh(1):", asinh1)
print("acosh(1):", acosh1)
print("acosh(2):", acosh2)
print("atanh(0):", atanh0)
print("atanh(0.5):", atanh_half)
print()

