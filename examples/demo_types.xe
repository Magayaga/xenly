// Type System Demo
print("Type System Demo")
print("================")

var x: number = 42
var s: string = "hello"
print("x =", x, "s =", s)

fn add(a: number, b: number): number {
    return a + b
}
print("add(10, 20) =", add(10, 20))

fn identity<T>(x: T): T {
    return x
}
print("identity(42) =", identity(42))

print("Demo complete!")
