// Complete Math Library Demo
print("=== Complete Math Library ===")
print()

import "math"

// Exponential Functions
print("1. Exponential")
print("exp(0):", math.exp(0))
print("exp(1):", math.exp(1))
print("exp(2):", math.exp(2))
print()

// Logarithmic Functions  
print("2. Logarithmic")
print("log(1):", math.log(1))
print("log(e):", math.log(math.E()))
print("log10(100):", math.log10(100))
print("log2(1024):", math.log2(1024))
print()

// Power Functions
print("3. Power")
print("pow(2, 10):", math.pow(2, 10))
print("sqrt(16):", math.sqrt(16))
print("cbrt(27):", math.cbrt(27))
print("hypot(3, 4):", math.hypot(3, 4))
print()

// Trigonometric
print("4. Trigonometric")
print("sin(0):", math.sin(0))
print("cos(0):", math.cos(0))
print("tan(0):", math.tan(0))
print()

// Arc Trigonometric
print("5. Arc Trigonometric")
print("asin(0):", math.asin(0))
print("asin(1):", math.asin(1))
print("acos(0):", math.acos(0))
print("acos(1):", math.acos(1))
print("atan(1):", math.atan(1))
print("atan2(1, 1):", math.atan2(1, 1))
print()

// Hyperbolic (manual implementation)
print("6. Hyperbolic")

const e_pos = math.exp(1)
const e_neg = math.exp(-1)
const sinh_1 = (e_pos - e_neg) / 2
const cosh_1 = (e_pos + e_neg) / 2

print("sinh(1):", sinh_1)
print("cosh(1):", cosh_1)
print()

// Inverse Hyperbolic
print("7. Inverse Hyperbolic")

const asinh_1 = math.log(1 + math.sqrt(2))
const acosh_2 = math.log(2 + math.sqrt(3))

print("asinh(1):", asinh_1)
print("acosh(2):", acosh_2)
print()

// Applications
print("8. Applications")

// Exponential growth
const initial = 1000.0
const rate = 0.05
const time = 10.0
const growth = initial * math.exp(rate * time)
print("Population growth:", growth)

// Distance
const dist = math.hypot(3, 4)
print("Distance (3,4):", dist)

// Angle
const angle = math.atan2(1, 1)
const degrees = angle * 180 / math.PI()
print("Angle (radians):", angle)
print("Angle (degrees):", degrees)

print()
